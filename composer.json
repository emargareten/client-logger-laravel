{
    "name": "emargareten/client-logger",
    "description": "Log HTTP client requests and responses in Laravel",
    "keywords": ["client-logger", "laravel", "log", "http"],
    "homepage": "https://github.com/emargareten/client-logger",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^7.0",
        "illuminate/http": "^10.0|^11.0",
        "illuminate/support": "^10.0|^11.0"
    },
    "require-dev": {
        "laravel/pint": "^1.13",
        "orchestra/testbench": "^8.21",
        "phpstan/phpstan": "^1.10",
        "phpunit/phpunit": "^10.5",
        "timacdonald/log-fake": "^2.1"
    },
    "autoload": {
        "psr-4": {
            "Emargareten\\ClientLogger\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Emargareten\\ClientLogger\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Emargareten\\ClientLogger\\ClientLoggerServiceProvider"
            ]
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
